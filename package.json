{
  "name": "what-to-watch",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Что посмотреть» от HTML Academy",
  "scripts": {
    "eslint": "npm run eslint.js && npm run eslint.ts",
    "eslint.js": "eslint --ext .jsx --ext .js src/",
    "eslint.ts": "eslint --config .eslintrc-ts.yml --ext .tsx src/",
    "test": "npm run eslint && npm run test.jest",
    "test.jest": "jest",
    "build": "webpack --mode production",
    "start": "webpack-dev-server"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@types/jest": "25.1.4",
    "@types/react": "16.9.27",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "axios-mock-adapter": "1.17.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "5.13.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-react": "7.18.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "react-test-renderer": "16.12.0",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "style-loader": "1.1.3",
    "ts-jest": "25.3.0",
    "ts-loader": "6.2.2",
    "typescript": "3.8.3",
    "utility-types": "3.10.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.2"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "axios": "0.19.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
